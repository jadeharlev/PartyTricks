    <ui:VisualElement name="Background" style="flex-grow: 1; background-color: rgb(156, 204, 223); align-items: center; justify-content: flex-start; flex-shrink: 0; width: 100%; height: 100%;">
        <ui:VisualElement name="GameLogo" style="flex-grow: 0; background-image: url(&quot;project://database/Assets/UI/Images/GameLogo.png?fileID=2800000&amp;guid=11c6b6203cbbf422c878192631be8927&amp;type=3#GameLogo&quot;); background-size: 1152px 200px; align-items: stretch; flex-shrink: 0; flex-basis: auto; width: 100%; height: 340px;" />
        <ui:VisualElement style="flex-grow: 1; width: 100%; flex-direction: row;">
            <ui:VisualElement name="LeftHalf" style="flex-grow: 0; width: 50%; align-items: center;">
